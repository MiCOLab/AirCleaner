@charset "utf-8";
html,
body {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
body {
  font: arial, "\5b8b\4f53", sans-serif, tahoma, "Microsoft YaHei";
}
* {
  margin: 0px;
  padding: 0px;
}
@font-face {
  font-family: mui;
  src: url("../fonts/mui.ttf");
}
li {
  list-style: none;
}
