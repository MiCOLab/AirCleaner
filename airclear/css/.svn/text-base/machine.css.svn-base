@charset "utf-8";
@font-face {
  font-family: mui;
  src: url("../fonts/mui.ttf");
}
* {
  margin:0; 
  padding:0;
  box-sizing:border-box;
  list-style:none;
  -wekit-tap-highlight-color:rgba(200,200,200,0.5)
}
html,
body {
  width: 100%;
  height: 100%;
  overflow: hidden;
  -webkit-tap-highlight-color:rgba(255,255,255,0);
   font-family: mui;
} 
* {
  margin: 0px;
  padding: 0px;
}

li {
  list-style: none;
}
.base .level0 {
  position: relative;
  width: 100%;
  height: 100%;
  background: url("../image/bg_sleep.png") no-repeat center;
  background-size: 100% 100%;
}
.base .center_bar {
  position: absolute;
  top: 50%;
  margin-top: -50%;
  padding: 0 32px;
  width: 100%;
}
.base .header {
  margin-bottom: 30px;
  width: 100%;
  height: 60px;
  text-align: center;
}
.base .header img {
  height: 100%;
}
.base .reg {
  width: 100%;
  color: #000;
}
.base .reg .p {
  padding: 0 18px;
  height: 54px;
  line-height: 54px;
  border-bottom: 1px solid rgba(0,0,0,.2);
}
.base .reg .p .cen_txt {
  padding: 0;
  text-align: center;
}
.base .reg input {
  padding-left: 40px; 
  width: 100%;
  height: 100%;
  font-size: 18px;
  background-color: transparent;
  border: 0px;
  outline: 0px;
  color: #666;
}
.base .reg input::-webkit-input-placeholder {
  color: rgba(0,0,0,.3);
}
.base .reg input:focus {
	opacity:1;
	color:#333;
}
.base .reg .lblwifi {
  background: url(../image/icon_wifi-01.png) no-repeat left;
  background-size: 23px auto;
}
.base .reg .lblpwd {
  background: url(../image/icon_password-01.png) no-repeat left;
  background-size: 23px auto;
}
.base .reg .lblcode {
  background: url(../image/icon_message-01.png) no-repeat left;
  background-size: 23px auto;
}
.base .reg .jl {
  display: inline-block;
  width: 25px;
}
.base .reg span {
  opacity: 0.2;
  float: left;
  height: 44px;
  display: inline-block;
  line-height: 44px;
}
.base .reg #btnSend {
  width: 120px;
  height: 42px;
  font-size: 18pt;
  color: #000000;
  opacity: 0.2 ;
  position: absolute;
  margin-left: -80px;
  border-radius: 5px;
}
.base .reg .btnReg {
  margin-top: 32px;
  width: 100%;
  height: 44px;
  line-height: 44px;
  font-size: 18px;
  letter-spacing: 2px;
  text-align: center;
  color: #fff;
  background-color: rgba(0,0,0,.2);
  border-radius: 5px;
}
.base .foot_bar {
  position: absolute;
  bottom: 18px;
  width: 100%;
  text-align: center;
}
.base .footclose {
  display: inline-block;
  width: 36px;
  height: 36px;
  background: url(../image/icon_cancel-01.png) no-repeat;
  background-size: contain;
}
.base .reg ::-webkit-input-placeholder {
    color:#666; 
    text-transform:uppercase;
}
.base .dialog {
  position: absolute;
  left: 0px;
  top: 0px;
  padding: 18px;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,.4);
  font-size: 18px;
  z-index: 99;
}
.base .dialog .mypwd,.base .dialog .myform {
  position: relative;
  top: 50%;
  margin-top: -230px;
  width: 100%;
  border-radius: 5px;
  background-color: #fff;
  color: #A49FA2;
  text-align: center;
  overflow: hidden;
}
.base .dialog .mypwd .inputarea,.base .dialog .myform .head {
  padding: 10px 0;
  font-size: 18px;
  color: #2ea6df;
  background-color: #f6f7f7;
  border-bottom: 1px solid #dcdddd;
}
.base .dialog .mypwd .inputarea p,.base .dialog .myform .content {
  padding: 20px 30px;
}
.base .dialog .mypwd .inputarea p,.base .dialog .myform .content p {
  margin-bottom: 10px;
  text-align: justify;
  font-size: 16px;
  color: #9e9f9f;
}
.base .dialog .mypwd .inputarea p,.base .dialog .myform .content p>span {
  display: inline-block;
  position: relative;
  top: -2px;
  margin-right: 5px;
  width: 20px;
  height: 20px;
  line-height: 20px;
  font-size: 14px;
  text-align: center;
  color: #fff;
  background-color: #dbdcdc;
  border-radius: 100%;
}
.base .dialog .mypwd .inputarea p,.base .dialog .myform .content .sketch {
  position: relative;
  margin-bottom: 80px;
}
.base .dialog .mypwd .inputarea p,.base .dialog .myform .content .sketch:last-child {
  margin-bottom: 0;
}
.base .dialog .mypwd .inputarea p,.base .dialog .myform .content .sketch img {
  height: 60px;
}
.base .dialog .mypwd .inputarea p,.base .dialog .myform .content .sketch .hand {
  position: absolute;
  top: 32px;
  left: 50%;
  margin-left: -16px;
  height: 78px;
  -webkit-animation: up 5s ease-out infinite;
          animation: up 5s ease-out infinite;
}
.base .dialog .mypwd .inputarea p,.base .dialog .myform .content .sketch .twinkle {
  display: inline-block;
  width: 60px;
  height: 60px;
  background-image: url(../image/twinkle-wifi-gray.png);
  background-repeat: no-repeat;
  background-size: contain;
  -webkit-animation: twinkle .8s ease infinite;
          animation: twinkle .8s ease infinite;
}
.base .dialog .myform .foot {
  padding: 10px 0;
  font-size: 18px;
  color: #595757;
  background-color: #f6f7f7;
  border-top: 1px solid #dcdddd;
}
@-webkit-keyframes up {
    0% {
        opacity: 0;
        -webkit-transform: translate3d(0, 30px, 0);
    }
    10% {
        opacity: 1;
        -webkit-transform: translate3d(0, 0, 0);
    }
    100% {
        opacity: 0;
        -webkit-transform: translate3d(0, 0, 0);
    }
}
@keyframes up {
    0% {
        opacity: 0;
        -webkit-transform: translate3d(0, 30px, 0);
    }
    10% {
        opacity: 1;
        -webkit-transform: translate3d(0, 0, 0);
    }
    100% {
        opacity: 0;
        -webkit-transform: translate3d(0, 0, 0);
    }
}
@-webkit-keyframes twinkle {
    0% {
        background-image: url(../image/twinkle-wifi-gray.png);
    }
    50% {
        background-image: url(../image/twinkle-wifi.png);
    }
    100% {
        background-image: url(../image/twinkle-wifi-gray.png);
    }
}
@keyframes twinkle {
    0% {
        background-image: url(../image/twinkle-wifi-gray.png);
    }
    50% {
        background-image: url(../image/twinkle-wifi.png);
    }
    100% {
        background-image: url(../image/twinkle-wifi-gray.png);
    }
}